extern print_i64(n: i64);

struct Foo {
    m: i64,
    n: i64,
    #transparent vtable: *Foo_Vtable,
}

struct Foo_Vtable {
    sum: fn(this: *Foo) i64,
    diff: fn(this: *Foo) i64,
}

fn main() i64 { 
    let foo = Foo {
        m: 1,
        n: 2,
        vtable: &_{
            sum: fn(this: *Foo) i64 { return this.m + this.n; },
            diff: fn(this: *Foo) i64 { return this.m - this.n; },
        },
    };

    print_i64(foo->sum);

    return 0;
}