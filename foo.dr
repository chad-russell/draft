extern print_i64(n: i64);
extern put_char(ch: u8);

fn print_string(s: string) {
    let i = 0;
    while i < s.len {
        put_char(s[i]);
        i = i + 1;
    }
}

module foo {
    fn bar() i64 {
        return 8;
    }
}

fn main() {
    "The answer is: " |> print_string;
    foo::bar() |> print_i64;
    "!\n" |> print_string;
}
