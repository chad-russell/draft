extern put_char(ch: u8);
extern print_str(ch: *u8, len: i64);
extern print_i64(n: i64);

fn print_string(s: string) {
    print_str(s.data, s.len);
    return;
}

struct Foo {
    x: i64,
    y: i64,
    
    const fn new(x: i64, y: i64) -> Foo {
        return Foo { x: x, y: y };
    }

    const fn sum(a: i32, this = &#this) -> i64 {
        return this.x + this.y;
    }
}

fn main() i64 {
    let f = Foo::new(1, 2);
    f.new(3, 4);
    f->sum(3, @); // f.sum(3, &f);
    
    return 0;
}